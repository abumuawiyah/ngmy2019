{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "atomic-component": {
      "root": "projects/atomic-component",
      "sourceRoot": "projects/atomic-component/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/atomic-component/tsconfig.lib.json",
            "project": "projects/atomic-component/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/atomic-component/src/test.ts",
            "tsConfig": "projects/atomic-component/tsconfig.spec.json",
            "karmaConfig": "projects/atomic-component/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/atomic-component/tsconfig.lib.json",
              "projects/atomic-component/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "chat-app": {
      "root": "projects/chat-app/",
      "sourceRoot": "projects/chat-app/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/chat-app",
            "index": "projects/chat-app/src/index.html",
            "main": "projects/chat-app/src/main.ts",
            "polyfills": "projects/chat-app/src/polyfills.ts",
            "tsConfig": "projects/chat-app/tsconfig.app.json",
            "assets": [
              "projects/chat-app/src/favicon.ico",
              "projects/chat-app/src/assets"
            ],
            "styles": [
              "projects/chat-app/src/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/chat-app/src/environments/environment.ts",
                  "with": "projects/chat-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "chat-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "chat-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "chat-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/chat-app/src/test.ts",
            "polyfills": "projects/chat-app/src/polyfills.ts",
            "tsConfig": "projects/chat-app/tsconfig.spec.json",
            "karmaConfig": "projects/chat-app/karma.conf.js",
            "assets": [
              "projects/chat-app/src/favicon.ico",
              "projects/chat-app/src/assets"
            ],
            "styles": [
              "projects/chat-app/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/chat-app/tsconfig.app.json",
              "projects/chat-app/tsconfig.spec.json",
              "projects/chat-app/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/chat-app/e2e/protractor.conf.js",
            "devServerTarget": "chat-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "chat-app:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "chat-app"
}